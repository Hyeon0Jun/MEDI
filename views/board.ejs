<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>의견 올리기</title>

    <!-- css -->
    <link rel="stylesheet" href="./assets/css/main.css" />
    <link rel="stylesheet" href="./assets/css/fonts.css" />
    <link rel="stylesheet" href="./assets/css/reset.css" />

    <!-- script -->
  </head>

  <body>
    <div id="wrap">
      <div class="header"></div>
      <div id="contents">
        <aside class="category">
          <p>병원 선택</p>
        </aside>
        <section class="board">
          <p>의견 올리기 게시판</p>
        </section>
        <footer id="footer"></footer>
      </div>
    </div>

    <!-- script -->
    <script
      type="text/javascript"
      src="//dapi.kakao.com/v2/maps/sdk.js?appkey=0519a42e5bfabdbb11f2ebf72e6949a6&libraries=services"
    ></script>
    <script src="./assets/js/header.js"></script>
    <script>
      const cookies = "<%= cookie %>";
      if (cookies === "false") {
        document.querySelector(".log").innerHTML = "로그인";
        document.querySelector(".log").href = "/login";
      } else {
        document.querySelector(".log").innerHTML = "로그아웃";
        document.querySelector(".log").href = "/logout";
      }
    </script>
  </body>
</html>
